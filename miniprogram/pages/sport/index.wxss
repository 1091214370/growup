/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  position: relative;
}
.userInfo {
  z-index: 11;
  margin-bottom: 20rpx;
}
.run-data-total {
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #e9e9e9;
  padding-left: 60rpx;
  text-align: left;
  font-weight: bolder;
  font-size: 16px;
}
.run-data-total-number {
  color: #00FA9A;
  font-size: 20px;
}
.echart_panel {
  width: 750rpx;
  height: 500rpx;
  margin-bottom: 40rpx;
}

.echart_panel_bar {
  width: 750rpx;
  height: 700rpx;
}
.mask {
  width: 100%;
  height: 100%;
  flex: 1;
  margin:0;
  padding: 0;
  z-index: 10;
  position: absolute;
  top: 0;
  left: 0;
}